.navbar-inner
	.container
		a.brand(href="/") Shackkarma
		.btn-group.pull-right
			a.btn.dropdown-toggle(data-toggle="dropdown",href="#")
				i.icon-user
				= user.username
				span.caret
			ul.dropdown-menu
				li: a(href="#") Profile
				li.divider
				li: a#logout(href="#") Logout
		.nav-collapse
			ul.nav
				for item in items
					li: a(href=item.href)= item.title
